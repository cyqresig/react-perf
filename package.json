    "scripts": {
        "eslint": "eslint . --quiet",
        "start": "cross-env NODE_ENV=development node ./scripts/dev.js",
        "test": "mocha test/.setup.js test/**/*-test.js",
        "test:watch": "mocha --watch test/.setup.js test/**/*-test.js",
        "nyc-test": "nyc mocha test/.setup.js test/**/*-test.js",
        "nyc-ava": "nyc ava",
        "nyc-report": "nyc report",
        "istanbul-test": "babel-istanbul cover _mocha -- test/.setup.js test/**/*-test.js -R spec",
        "istanbul-test:watch": "istanbul cover _mocha --watch test/.setup.js test/**/*-test.js",
        "karma": "karma start --single-run --browsers PhantomJS",
        "karma:watch": "karma start"
    },
    "ava": {
        "files": [
            "test-ava/**/*-test.js"
        ],
        "require": [
            "babel-register",
            "./test-ava/helpers/setup-browser-env.js"
        ],
        "concurrency": 5,
        "failFast": true,
        "failWithoutAssertions": false,
        "babel": "inherit"
    }
